[Script]

# Unlock Gold and set badge
revenuecat = type=http-response, pattern=^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$), script-path=https://raw.githubusercontent.com/hnhat/locket-gold/main/js/Locket_HNhat.js, requires-body=true, max-size=-1, timeout=60

# Clean headers (optional, tránh bị nhận diện)
